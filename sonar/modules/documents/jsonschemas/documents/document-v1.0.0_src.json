        },
        "propertiesOrder": [
          "publicNote",
          "document"
        ],
        "required": [
          "document",
          "publicNote"
        ]
      },
      "form": {
        "hide": true
      }
    },
    "specificCollections": {
      "title": "Collections",
      "type": "array",
      "minItems": 1,
      "items": {
        "title": "Collection",
        "type": "string",
        "minLength": 1
      },
      "form": {
        "hide": true,
        "navigation": {
          "essential": true
        }
      }
    },
    "classification": {
      "title": "Classifications",
      "type": "array",
      "minItems": 1,
      "items": {
        "title": "Classification",
        "type": "object",
        "oneOf": [
          {
            "title": "bf:ClassificationUdc",
            "properties": {
              "type": {
                "title": "Type",
                "type": "string",
                "enum": [
                  "bf:ClassificationUdc"
                ],
                "form": {
                  "options": [
                    {
                      "label": "bf:ClassificationUdc",
                      "value": "bf:ClassificationUdc"
                    }
                  ]
                }
              },
              "classificationPortion": {
                "$ref": "classification-v1.0.0.json"
              }
            },
            "propertiesOrder": [
              "type",
              "classificationPortion"
            ],
            "required": [
              "type",
              "classificationPortion"
            ]
          },
          {
            "title": "bf:ClassificationDdc",
            "properties": {
              "type": {
                "title": "Type",
                "type": "string",
                "enum": [
                  "bf:ClassificationDdc"
                ],
                "form": {
                  "options": [
                    {
                      "label": "bf:ClassificationDdc",
                      "value": "bf:ClassificationDdc"
                    }
                  ]
                }
              },
              "classificationPortion": {
                "title": "Classification portion",
                "type": "string",
                "minLength": 1
              }
            },
            "propertiesOrder": [
              "type",
              "classificationPortion"
            ],
            "required": [
              "type",
              "classificationPortion"
            ]
          }
        ]
      },
      "form": {
        "hide": true,
        "navigation": {
          "essential": true
        }
      }
    },
    "contentNote": {
      "title": "Content notes",
      "type": "array",
      "minItems": 1,
      "items": {
        "title": "Content note",
        "type": "string",
        "minLength": 1
      },
      "form": {
        "hide": true
      }
    },
    "dissertation": {
      "title": "Dissertation",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "degree": {
          "title": "Degree",
          "type": "string",
          "minLength": 1
        },
        "jury_note": {
          "title": "Jury note",
          "type": "string",
          "minLength": 1
        },
        "grantingInstitution": {
          "title": "Granting institution",
          "type": "string",
          "minLength": 1
        },
        "date": {
          "title": "Date",
          "type": "string",
          "pattern": "^[0-9]{4}(-[0-9]{2}-[0-9]{2})?$",
          "form": {
            "placeholder": "Example: 2019 or 2019-05-05"
          }
        }
      },
      "propertiesOrder": [
        "degree",
        "grantingInstitution",
        "date",
        "jury_note"
      ],
      "required": [
        "degree"
      ],
      "form": {
        "hide": true
      },
      "hideExpression": "!['coar:c_46ec', 'coar:c_7a1f', 'coar:c_db06', 'coar:c_bdcc', 'habilitation_thesis', 'advanced_studies_thesis', 'other'].includes(field.parent.model.documentType)"
    },
    "usageAndAccessPolicy": {
      "title": "Usage and access policies",
      "type": "array",
      "minItems": 1,
      "items": {
        "title": "Usage and access policy",
