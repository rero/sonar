        "properties": {
          "document": {
            "title": "Document",
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "electronicLocator": {
                "title": "URL",
                "type": "string",
                "minLength": 1
              }
            },
            "required": [
              "electronicLocator"
            ]
          },
          "publicNote": {
            "title": "Public note",
            "type": "string",
            "minLength": 1,
            "form": {
              "placeholder": "Example: Published version"
            }
