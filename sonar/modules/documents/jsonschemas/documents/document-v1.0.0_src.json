        ]
      },
      "form": {
        "hide": true,
        "navigation": {
          "essential": true
        }
      }
    },
    "harvested": {
      "title": "Harvested",
      "description": "Document is harvested or not, will disable record edition or similar.",
      "type": "boolean",
      "default": false
    },
    "otherEdition": {
      "title": "Other electronic versions",
      "type": "array",
      "minItems": 1,
      "items": {
        "title": "Other electronic version",
        "type": "object",
